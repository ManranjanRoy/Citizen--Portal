<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorPrimary"
    tools:context=".Activity.TradeForm4ctivity">
    <RelativeLayout
        android:id="@+id/r1"
        android:gravity="center_vertical"
        android:layout_width="match_parent"
        android:layout_height="56dp">
        <LinearLayout
            android:gravity="center_vertical"
            android:layout_width="wrap_content"
            android:layout_height="match_parent">
            <ImageView
                android:id="@+id/back"
                android:layout_marginLeft="10dp"
                android:src="@drawable/ic_arrow_back_black_24dp"
                android:layout_width="50dp"
                android:layout_height="30dp"/>
        </LinearLayout>
        <TextView
            android:textStyle="bold"
            android:gravity="center"
            android:textAlignment="center"
            android:textSize="20dp"
            android:textColor="@color/white"
            android:text="Review &amp; Submit"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
    </RelativeLayout>
    <ScrollView
        android:layout_below="@id/r1"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <RelativeLayout
            android:paddingTop="10dp"
            android:paddingBottom="20dp"
            android:layout_marginHorizontal="10dp"
            android:gravity="center"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <LinearLayout
                android:paddingVertical="10dp"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/roundedcorner"
                android:orientation="vertical">
            <!--Spinner widget-->
                <!--Spinner widget-->
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/mobilenolayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                    app:errorEnabled="true"
                    app:hintEnabled="true">

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/holdingno"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Assesse/Holding No."
                        android:inputType="number"
                        android:enabled="false"
                        android:textColor="@color/black"
                        android:maxLength="10"
                        android:textSize="14dp" />

                </com.google.android.material.textfield.TextInputLayout>

                <com.google.android.material.textfield.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                    app:errorEnabled="true"
                    app:hintEnabled="true">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/ownername"
                        android:enabled="false"
                        android:textColor="@color/black"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:focusedByDefault="true"
                        android:hint="Owner name"
                        android:textSize="14dp" />

                </com.google.android.material.textfield.TextInputLayout>

                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/namelayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                    app:errorEnabled="true"
                    app:hintEnabled="true">

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/streetaddress"
                        android:enabled="false"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Street/Road Name/Address"
                        android:textColor="@color/black"
                        android:textSize="14dp" />

                </com.google.android.material.textfield.TextInputLayout>



                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/emaillayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                    app:errorEnabled="true"
                    app:hintEnabled="true">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/locality"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Locality"
                        android:enabled="false"
                        android:textColor="@color/black"
                        android:inputType="text"
                        android:textSize="14dp" />
                </com.google.android.material.textfield.TextInputLayout>
                <TextView
                    android:layout_marginBottom="3dp"
                    android:text="Ward Number"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>
                <LinearLayout
                    android:layout_marginBottom="5dp"
                    android:background="@drawable/spinnerbox"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <Spinner
                        android:textAlignment="center"
                        android:id="@+id/wardno"
                        android:layout_width="match_parent"
                        android:layout_height="45dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />
                </LinearLayout>
                <LinearLayout
                    android:layout_marginBottom="5dp"
                    android:orientation="vertical"
                    android:layout_marginTop="5dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:text="Type of Tax"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"/>
                    <RadioGroup
                        android:orientation="horizontal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/radioGroup">

                        <RadioButton
                            android:onClick="radioclick"
                            android:id="@+id/radioyes"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Property Tax"
                            android:checked="true"
                            android:textSize="15dp" />

                        <RadioButton
                            android:onClick="radioclick"
                            android:id="@+id/radiono"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Occupier Tax"
                            android:checked="false"
                            android:textSize="15dp" />
                    </RadioGroup>
                </LinearLayout>

                <com.google.android.material.textfield.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                    app:errorEnabled="true"
                    app:hintEnabled="true">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/ammountpaidfortax"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Amount paid for property tax (Rs)"
                        android:textColor="@color/black"
                        android:inputType="number"
                        android:enabled="false"
                        android:textSize="14dp" />
                </com.google.android.material.textfield.TextInputLayout>
                <TextView
                    android:layout_marginBottom="3dp"
                    android:text="Type of relation with land"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>
                <LinearLayout
                    android:layout_marginBottom="15dp"
                    android:background="@drawable/spinnerbox"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <Spinner
                        android:textAlignment="center"
                        android:id="@+id/typesofrelation"
                        android:layout_width="match_parent"
                        android:layout_height="45dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />
                </LinearLayout>
                <TextView
                    android:layout_marginBottom="3dp"
                    android:text="Type of Business"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>
                <LinearLayout
                    android:layout_marginBottom="15dp"
                    android:background="@drawable/spinnerbox"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <Spinner
                        android:textAlignment="center"
                        android:id="@+id/typeofbusiness"
                        android:layout_width="match_parent"
                        android:layout_height="45dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />
                </LinearLayout>
                <TextView
                    android:layout_marginBottom="3dp"
                    android:text="For The Year"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>
                <LinearLayout
                    android:layout_marginBottom="15dp"
                    android:background="@drawable/spinnerbox"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <Spinner
                        android:textAlignment="center"
                        android:id="@+id/fortheyear"
                        android:layout_width="match_parent"
                        android:layout_height="45dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />
                </LinearLayout>
                <!--Spinner widget-->
                <com.google.android.material.textfield.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                    app:errorEnabled="true"
                    app:hintEnabled="true">

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/nameofrirm"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Name of the firm/company"
                        android:inputType="text"
                        android:textColor="@color/black"
                        android:maxLength="10"
                        android:textSize="14dp" />

                </com.google.android.material.textfield.TextInputLayout>
                <LinearLayout
                    android:gravity="center_vertical"
                    android:layout_marginBottom="15dp"
                    android:background="@drawable/spinnerbox"
                    android:layout_width="match_parent"
                    android:layout_height="45dp">
                    <TextView
                        android:textColor="@color/black"
                        android:drawableRight="@android:drawable/ic_menu_my_calendar"
                        android:gravity="center"
                        android:hint="Date of Commenencement"
                        android:textSize="14dp"
                        android:id="@+id/Dateofcommen"
                        android:layout_width="match_parent"
                        android:layout_height="45dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />
                </LinearLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/textcompany"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                    app:errorEnabled="true"
                    app:hintEnabled="true">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/investmentofcapital"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:focusedByDefault="true"
                        android:inputType="number"
                        android:hint="Investment of Capital(Rs)"
                        android:textSize="14dp" />

                </com.google.android.material.textfield.TextInputLayout>
                <TextView
                    android:visibility="gone"
                    android:layout_marginBottom="3dp"
                    android:text="Trade Type"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>
                <LinearLayout
                    android:visibility="gone"
                    android:layout_marginBottom="5dp"
                    android:background="@drawable/spinnerbox"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <Spinner
                        android:textAlignment="center"
                        android:id="@+id/tradetype"
                        android:layout_width="match_parent"
                        android:layout_height="45dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />
                </LinearLayout>
                <TextView
                    android:visibility="gone"
                    android:layout_marginBottom="2dp"
                    android:text="Nature of trade"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>
                <com.google.android.material.textfield.TextInputLayout
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                    app:errorEnabled="true"
                    app:hintEnabled="true">

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/fname"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Nature of Trade "
                        android:textColor="@color/black"
                        android:textSize="14dp" />



                </com.google.android.material.textfield.TextInputLayout>
                <LinearLayout
                    android:visibility="gone"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="5dp"
                    android:background="@drawable/spinnerbox"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <AutoCompleteTextView
                        android:paddingLeft="10dp"
                        android:background="@android:color/transparent"
                        android:id="@+id/autoCompleteTextView"
                        android:layout_width="match_parent"
                        android:layout_height="45dp"
                        android:ems="10"
                        android:text="">
                    </AutoCompleteTextView>
                </LinearLayout>
                <LinearLayout
                    android:background="#EEE"
                    android:id="@+id/rlayout"
                    android:gravity="center"
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <TableLayout
                            android:layout_marginBottom="5dp"
                            android:weightSum="3"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">
                            <TableRow>
                                <TextView
                                    android:textAlignment="center"
                                    android:textColor="@color/black"
                                    android:textStyle="bold"
                                    android:gravity="center"
                                    android:layout_weight="1"
                                    android:text=" Trade Size "
                                    android:layout_width="100dp"
                                    android:layout_height="wrap_content" />

                                <TextView
                                    android:textAlignment="center"
                                    android:textColor="@color/black"
                                    android:textStyle="bold"
                                    android:text="Nature Of Trade "
                                    android:gravity="center"
                                    android:layout_weight="1"
                                    android:layout_width="125dp"
                                    android:layout_height="wrap_content"
                                    />
                                <TextView
                                    android:visibility="gone"
                                    android:textAlignment="center"
                                    android:textColor="@color/black"
                                    android:textStyle="bold"
                                    android:text="Action"
                                    android:gravity="center"
                                    android:layout_weight="1"
                                    android:layout_width="100dp"
                                    android:layout_height="wrap_content" />
                            </TableRow>
                        </TableLayout>
                    </LinearLayout>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@color/colorPrimary"/>
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <androidx.recyclerview.widget.RecyclerView
                            android:layout_marginTop="5dp"
                            android:id="@+id/tradelist"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"/>
                    </RelativeLayout>
                    <View
                        android:visibility="gone"
                        android:layout_marginTop="2dp"
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@color/colorPrimary"/>
                </LinearLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:layout_marginTop="5dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                    app:errorEnabled="true"
                    app:hintEnabled="true">

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/gstin"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="GST IN"
                        android:textColor="@color/black"
                        android:textSize="14dp" />

                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                    app:errorEnabled="true"
                    app:hintEnabled="true">

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/companypancard"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Company PAN Card"
                        android:textColor="@color/black"
                        android:textSize="14dp" />

                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                    app:errorEnabled="true"
                    app:hintEnabled="true">

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/contactnofirst"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Contact Number"
                        android:inputType="number"
                        android:maxLength="10"
                        android:textColor="@color/black"
                        android:textSize="14dp" />

                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                    app:errorEnabled="true"
                    app:hintEnabled="true">

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/contactaddress"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:gravity="top|left"
                        android:inputType="textEmailAddress|textMultiLine"
                        android:lines="3"
                        android:hint="Contact Address"
                        android:scrollHorizontally="false"
                        android:scrollbars="vertical"/>

                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                    app:errorEnabled="true"
                    app:hintEnabled="true">

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/workshopaddress"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:gravity="top|left"
                        android:inputType="textEmailAddress|textMultiLine"
                        android:lines="3"
                        android:hint="WorkShop Address"
                        android:scrollHorizontally="false"
                        android:scrollbars="vertical"/>

                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                    app:errorEnabled="true"
                    app:hintEnabled="true">

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/godownaddress"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:gravity="top|left"
                        android:inputType="textEmailAddress|textMultiLine"
                        android:lines="3"
                        android:hint="Godown Address"
                        android:scrollHorizontally="false"
                        android:scrollbars="vertical"/>

                </com.google.android.material.textfield.TextInputLayout>

                <LinearLayout
                    android:visibility="gone"
                    android:background="#EEE"
                    android:id="@+id/rlayoutforowner"
                    android:gravity="center"
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <androidx.core.widget.NestedScrollView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">
                            <HorizontalScrollView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">
                                <TableLayout
                                    android:gravity="center_vertical"
                                    android:layout_marginBottom="5dp"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content">
                                    <TableRow>
                                        <TextView
                                            android:textAlignment="center"
                                            android:textColor="@color/black"
                                            android:textStyle="bold"
                                            android:gravity="center"
                                            android:layout_weight="1"
                                            android:text=" Name "
                                            android:layout_width="100dp"
                                            android:layout_height="wrap_content" />

                                        <TextView
                                            android:textAlignment="center"
                                            android:textColor="@color/black"
                                            android:textStyle="bold"
                                            android:text="Father/Husband Name"
                                            android:gravity="center"
                                            android:layout_weight="1"
                                            android:layout_width="150dp"
                                            android:layout_height="wrap_content"
                                            />
                                        <TextView
                                            android:textAlignment="center"
                                            android:textColor="@color/black"
                                            android:textStyle="bold"
                                            android:text="Contact no"
                                            android:gravity="center"
                                            android:layout_weight="1"
                                            android:layout_width="100dp"
                                            android:layout_height="wrap_content" />
                                        <TextView
                                            android:textAlignment="center"
                                            android:textColor="@color/black"
                                            android:textStyle="bold"
                                            android:text="Address"
                                            android:gravity="center"
                                            android:layout_weight="1"
                                            android:layout_width="150dp"
                                            android:layout_height="wrap_content" />
                                        <TextView
                                            android:textAlignment="center"
                                            android:textColor="@color/black"
                                            android:textStyle="bold"
                                            android:text="Action"
                                            android:gravity="center"
                                            android:layout_weight="1"
                                            android:layout_width="50dp"
                                            android:layout_height="wrap_content" />
                                    </TableRow>
                                    <View
                                        android:visibility="visible"
                                        android:layout_marginTop="2dp"
                                        android:layout_width="match_parent"
                                        android:layout_height="1dp"
                                        android:background="@color/colorPrimary"/>
                                    <androidx.recyclerview.widget.RecyclerView
                                        android:scrollbars="vertical"
                                        android:layout_marginTop="5dp"
                                        android:id="@+id/ownerlist"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"/>
                                </TableLayout>
                            </HorizontalScrollView>
                        </LinearLayout>
                    </androidx.core.widget.NestedScrollView>
                    <View
                        android:visibility="gone"
                        android:layout_marginTop="2dp"
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@color/colorPrimary"/>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/rlayoutforpartner"
                    android:visibility="gone"
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/totaldirectorlayout"
                        android:visibility="gone"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                        app:errorEnabled="true"
                        app:hintEnabled="true">

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/noofdirector"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textColor="@color/black"
                            android:textSize="14dp" />

                    </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                    app:errorEnabled="true"

                    app:hintEnabled="true">

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/nameofdirector"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/black"
                        android:textSize="14dp" />

                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                    app:errorEnabled="true"
                    app:hintEnabled="true">

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/fathersname"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Father/Husband Name"
                        android:textColor="@color/black"
                        android:textSize="14dp" />

                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                    app:errorEnabled="true"
                    app:hintEnabled="true">

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/address"
                        android:layout_width="match_parent"
